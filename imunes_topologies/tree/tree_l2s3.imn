link l0 {
    delay 2000
    nodes {n3 n1}
    bandwidth 100000000
}

link l1 {
    delay 2000
    nodes {n1 n0}
    bandwidth 100000000
}

link l2 {
    delay 2000
    nodes {n0 n2}
    bandwidth 100000000
}

link l4 {
    delay 2000
    nodes {n5 n2}
    bandwidth 100000000
}

link l5 {
    delay 2000
    nodes {n2 n6}
    bandwidth 100000000
}

link l6 {
    delay 2000
    nodes {n1 n4}
    bandwidth 100000000
}

canvas c0 {
    name {Canvas0}
}

option show {
    interface_names yes
    ip_addresses yes
    ipv6_addresses yes
    node_labels yes
    link_labels yes
    background_images no
    annotations yes
    hostsAutoAssign no
    grid yes
    iconSize normal
    zoom 1.0
}

